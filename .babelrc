{
    "env": {
      "development": {
        "plugins": [
          "captains-log"
        ]
      }
    },
    "presets": [
        "@babel/preset-react",
        ["@babel/preset-env", {
            "targets": {
                "browsers": [
                    "last 2 versions",
                    "Firefox 52",
                    "safari >= 7",
                    "IE >= 11"
                ]
            }
        }]
    ],
    "plugins": [
        "lodash",
        "@babel/plugin-syntax-dynamic-import",
        "@babel/plugin-transform-react-jsx",
        "@babel/plugin-proposal-object-rest-spread",
        ["@babel/plugin-transform-runtime", {
            "regenerator": true
        }]
    ]
}
